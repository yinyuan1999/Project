<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">    
<!--     
    namespace：必须与对应的接口全类名一致  UserMapper.java  
    id ：必须与对应接口的某个对应的方法名一致即必须要和UserMapper.java接口中的方法同名。  
 -->    
 <mapper namespace="com.yy.mapper.GinfoMapper">  
   
    <insert id="save" parameterType="Ginfo">  
        insert into ginfo(ginfo_name,ginfo_typeid,ginfo_price,ginfo_store) 
        values(#{name},#{typeid},#{price},#{store})  
    </insert>  
      
    <update id="update" parameterType="Ginfo">    
        update ginfo set ginfo_name=#{name},ginfo_typeid=#{typeid},
        ginfo_price=#{price},ginfo_store=#{store}
         where ginfo_id=#{id}    
    </update>   
      
    <delete id="delete" parameterType="int">    
        delete from ginfo where ginfo_id=#{id}    
    </delete>    
        
     <!-- mybsits_config中配置的alias类别名,也可直接配置resultType为类路径 -->      
    <select id="findById" parameterType="int" resultType="Ginfo">    
        select ginfo_id id,ginfo_name name,ginfo_typeid typeid,
        ginfo_price price,ginfo_store store from ginfo where ginfo_id=#{id}   
    </select>    
        
    <select id="findAll" resultType="Ginfo">    
        select ginfo_id id,ginfo_name name,ginfo_typeid typeid,
        ginfo_price price,ginfo_store store from ginfo    
    </select>   
      
 </mapper>